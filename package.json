    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "svelte": "^3.37.0",
    "svelte-check": "^2.1.0",
    "svelte-jester": "^2.0.1",
    "svelte-preprocess": "^4.7.4",
    "ts-jest": "^27.0.4",
    "tslib": "^2.2.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  },
  "dependencies": {
    "localforage": "^1.9.0",
    "twind": "^0.16.16",
    "y-indexeddb": "^9.0.6",
    "y-webrtc": "^10.2.0",
    "yjs": "^13.5.12"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
